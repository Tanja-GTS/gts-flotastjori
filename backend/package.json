{
  "name": "fleet-scheduler-backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:tsnode": "ts-node-dev --respawn --transpile-only src/server.ts",
    "graph:login": "tsx src/scripts/graphLogin.ts",
    "graph:test": "tsx src/scripts/graphTest.ts",
    "generate:shifts": "tsx src/scripts/generateShiftsCli.ts",
    "start": "tsx src/server.ts",
    "start:prod": "SERVE_FRONTEND=true tsx src/server.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@azure/msal-node": "^3.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.5",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
